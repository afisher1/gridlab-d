//  2 bus simple system
// Checks to make sure "from/to" of a link is actually a node
// This one links to a switch (which would pass the phases check)

clock {
	timezone "PST+8PDT";
	starttime '2001-01-01 00:00:00 PST';
	stoptime '2001-01-01 00:00:05 PST';
	
}

#set suppress_repeat_messages=1
//#set profiler=1

module powerflow {
	solver_method NR;
}

// Phase Conductor for 601: 556,500 26/7 ACSR
object overhead_line_conductor {
	name olc6010;
	geometric_mean_radius 0.031300;
	diameter 0.927 in;
	resistance 0.185900;
}

// Phase Conductor for 602: 4/0 6/1 ACSR
object overhead_line_conductor {
	name olc6020;
	geometric_mean_radius 0.00814;
	diameter 0.56 in;
	resistance 0.592000;
}

// Overhead line configurations
object line_spacing {
	name ls500601;	//With capacitance ability
	distance_AB 2.5;
	distance_AC 4.5;
	distance_BC 7.0;
	distance_BN 5.656854;
	distance_AN 4.272002;
	distance_CN 5.0;
	distance_AE 28.0;
	distance_BE 28.0;
	distance_CE 28.0;
	distance_NE 24.0;
}

object line_configuration {
	name lc601;
	conductor_A olc6010;
	conductor_B olc6010;
	conductor_C olc6010;
	conductor_N olc6020;
	spacing ls500601;
}

object overhead_line {
	phases "ABCN";
	name node1_to_node2;
	from testSwitch; //was node1a;
	to node2;
	length 10.0 mile; //15.0 mile;
	configuration lc601;
}

object switch {
	name testSwitch;
	phases ABCN;
	from node1;
	to node1a;
	status CLOSED;
}

object meter {
	name node1;
	phases "ABCN";
	bustype SWING;
	voltage_A 2401.7771;
	voltage_B -1200.8886-2080.000j;
	voltage_C -1200.8886+2080.000j;
	nominal_voltage 2401.7771;
}

object node {
	phases ABCN;
	name node1a;
	nominal_voltage 2401.7771;
}

object load {
	name node2;
	phases ABCN;
	nominal_voltage 2401.7771;
	constant_impedance_A 34.9608 +17.4804j;
	constant_impedance_B 87.4020 +43.7010j;
	constant_impedance_C 21.8505 +10.9253j;
}
